<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/listViewProposalMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:background="@color/black"
        android:id = "@+id/showProposalListBack">
        
    	<ImageView
	        android:id="@+id/imageView2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:src="@drawable/oa_left" />
    	
    	<ImageView
	        android:id="@+id/imageView1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:src="@drawable/open_antrag" />
	    
	    <TextView
	        android:id="@+id/textHeadline"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/setFilter"
   	        android:textColor ="@color/lightGrey"
	        android:textAppearance="?android:attr/textAppearanceLarge" />
        
    </LinearLayout>

    <ListView
        android:id="@+id/listViewProposal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>
